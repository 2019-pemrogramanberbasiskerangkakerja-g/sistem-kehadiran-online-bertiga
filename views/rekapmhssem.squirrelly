<!DOCTYPE html>
<html lang="en">
<head>
	<title>Rekap Mahasiswa Semester</title>
	{{ include(head)/ }}
</head>
<body>
	
	<div class="limiter">
		<div class="container-table100">
			<div class="wrap-table100">
				<div class="table100">
					<table>
						<thead>
							<tr class="table100-head">
								<th class="column1">Mahasiswa ID</th>
								<th class="column2">Pertemuan</th>
								<th class="column3">Waktu Absen</th>
							</tr>
						</thead>
						<tbody>
							{{each(options.isi_data)}}
								<tr>
									<td class="column1">{{@this.mahasiswa_id}}</td>
									<td class="column2">{{@this.pertemuan_matakuliah_id}}</td>
									<td class="column3">{{@this.tanggal_waktu}}</td>
								</tr>
							{{/each}}
						</tbody>
					</table>
				</div>
			</div>
		</div>
	</div>
 {{ include(foot)/ }}
</body>
</html>