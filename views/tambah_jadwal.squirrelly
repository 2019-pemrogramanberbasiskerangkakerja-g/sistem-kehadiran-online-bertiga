<!DOCTYPE html>
<html lang="en">
<head>

  {{include(head)/}}

</head>
<body>

{{include(navbar)/}}

<!-- Content here -->
    {{ if(options.status) }}
        {{ if(options.status == 'sukses') }}
            <div class="alert alert-success text-center">
                {{ pesan }}
            </div>
        {{ #else }}
            <div class="alert alert-danger">
                {{ pesan }}
            </div>
        {{ /if }}
    {{ /if }}

    <div class="card">
        <div class="card-body">
            <div class="col-md-6 offset-md-3">
                <form action="/vtambahjadwalPost" method="post">
                    <div class="form-group">
                        <label for="matakuliah_id">ID Matkul</label>
                        <input type="text" name="matakuliah_id" class="form-control" required>
                    </div>
                    <div class="form-group">
                        <label for="pertemuan">Pertemuan</label>
                        <input type="text" name="pertemuan" class="form-control" required>
                    </div>
                    <div class="form-group">
                        <label for="ruangan">Ruangan</label>
                        <input type="text" name="ruangan" class="form-control" required>
                    </div>
                    <div class="form-group">
                        <label for="jam_mulai">Jam Mulai</label>
                        <input type="text" name="jam_mulai" class="form-control" required>
                    </div>
                    <div class="form-group">
                        <label for="jam_selesai">Jam Selesai</label>
                        <input type="text" name="jam_selesai" class="form-control" required>
                    </div>
                    <div class="form-group">
                        <input type="submit" class="btn btn-primary" value="Submit">
                    </div>
                </form>
            </div>
        </div>
    </div>

{{include(foot)/}}

</body>
</html>
